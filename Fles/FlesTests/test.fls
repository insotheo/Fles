fn printInfoAboutPerson(name : string, age : int, phrase : string){
    std_println("Hi! My name is " + name + "! And I'm " + age + ". I say: '" + phrase + "'.");
}

fn main(){
    //Person
    let name : string = "Person";
    let age : int = 99;
    printInfoAboutPerson(name, age, "Hello, World" + "!");
}